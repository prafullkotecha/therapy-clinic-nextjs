# Keycloak Authentication
KEYCLOAK_URL=http://localhost:8080
KEYCLOAK_REALM=therapy-clinic
KEYCLOAK_CLIENT_ID=therapy-clinic-app
KEYCLOAK_CLIENT_SECRET=your_client_secret_here

# NextAuth
AUTH_SECRET=your-secret-key-min-32-chars-change-in-production
AUTH_URL=http://localhost:3000

# PostHog
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com

# Database
# Local PostgreSQL (via docker-compose up)
# For production, use AWS RDS PostgreSQL with Multi-AZ and encryption
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:5432/therapy_clinic_dev

# Next.js
NEXT_TELEMETRY_DISABLED=1

######## [BEGIN] SENSITIVE DATA ######## For security reason, don't update the following variables (secret key) directly in this file.
